{
  "name": "ai-database",
  "version": "0.2.0",
  "description": "AI-powered database interface primitives with mdxld conventions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tests": {
      "import": "./dist/tests.js",
      "types": "./dist/tests.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ai-functions": "workspace:*",
    "mdxld": "^1.0.1",
    "rpc.do": "^0.1.0"
  },
  "peerDependencies": {
    "vitest": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    }
  },
  "keywords": [
    "ai",
    "database",
    "linked-data",
    "mdxld",
    "primitives"
  ],
  "license": "MIT"
}
