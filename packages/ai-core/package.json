{
  "name": "ai-core",
  "version": "1.0.0",
  "description": "Core AI primitives: generate, AIPromise, template literals, and context management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest",
    "test:unit": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ai": "^5.0.0",
    "ai-providers": "workspace:^",
    "language-models": "workspace:*",
    "yaml": "^2.8.0",
    "zod": "^3.23.0"
  },
  "keywords": [
    "ai",
    "primitives",
    "generate",
    "promise",
    "template",
    "context"
  ],
  "license": "MIT",
  "devDependencies": {
    "vitest": "^2.1.0"
  }
}
