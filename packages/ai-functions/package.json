{
  "name": "ai-functions",
  "version": "0.0.1",
  "description": "Core AI primitives for building intelligent applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./rpc": {
      "import": "./dist/rpc/index.js",
      "types": "./dist/rpc/index.d.ts"
    },
    "./embeddings": {
      "import": "./dist/embeddings.js",
      "types": "./dist/embeddings.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ai": "^4.0.0",
    "capnweb": "^0.0.3"
  },
  "optionalDependencies": {
    "oauth.do": "^0.0.1"
  },
  "keywords": ["ai", "functions", "rpc", "capnweb", "embeddings", "oauth", "primitives"],
  "license": "MIT"
}
