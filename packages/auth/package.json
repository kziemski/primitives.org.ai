{
  "name": "@mdxe/auth",
  "version": "0.0.1",
  "description": "Authentication for MDX applications with WorkOS integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware.js",
      "types": "./dist/middleware.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./jsx": {
      "import": "./dist/jsx.js",
      "types": "./dist/jsx.d.ts"
    },
    "./context": {
      "import": "./dist/context.js",
      "types": "./dist/context.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@workos-inc/node": "^7.0.0",
    "iron-session": "^8.0.0"
  },
  "peerDependencies": {
    "hono": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "hono": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hono/node-server": "^1.13.0",
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.0.0",
    "hono": "^4.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "keywords": [
    "auth",
    "workos",
    "oauth",
    "sso",
    "authentication",
    "hono",
    "iron-session"
  ],
  "license": "MIT"
}
